{
	"name": "webpack5-vue3-for-remato-wordpress",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "webpack --config config/webpack.dev.js --node-env=development --watch",
		"build": "webpack --config config/webpack.prod.js --node-env=production",
		"lint": "yarn lint:js && yarn lint:scss",
		"lint:js": "eslint \"**/*.{vue,ts,tsx,js}\"",
		"lint:scss": "stylelint \"**/*.{vue,scss}\"",
		"prettier:check": "prettier --list-different \"src/**/*.{vue,ts,tsx,js,scss}\"",
		"prettier:fix": "prettier --write \"src/*.{vue,ts,tsx,js,scss}\"",
		"clean:modules": "rm -rf node_modules"
	},
	"devDependencies": {
		"@babel/core": "^7.16.7",
		"@babel/plugin-transform-runtime": "^7.16.8",
		"@babel/preset-env": "^7.16.8",
		"@babel/preset-typescript": "^7.16.7",
		"@remato/eslint-config": "^1.8.0",
		"@remato/prettier-config": "^1.0.0",
		"@remato/stylelint-config": "^1.8.1",
		"@vue/compiler-sfc": "^3.2.26",
		"babel-loader": "^8.2.3",
		"clean-webpack-plugin": "^4.0.0",
		"css-loader": "^6.5.1",
		"css-minimizer-webpack-plugin": "^3.3.1",
		"eslint": "^8.6.0",
		"eslint-webpack-plugin": "^3.1.1",
		"fork-ts-checker-webpack-plugin": "^6.5.0",
		"mini-css-extract-plugin": "^2.4.6",
		"postcss-loader": "^6.2.1",
		"postcss-preset-env": "^7.2.0",
		"prettier": "^2.5.1",
		"sass": "^1.47.0",
		"sass-loader": "^12.4.0",
		"stylelint": "^13.0.0",
		"stylelint-webpack-plugin": "^3.1.0",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.4",
		"vue-loader": "^17.0.0",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.9.1",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"@babel/runtime": "^7.16.7",
		"@babel/runtime-corejs3": "^7.16.8",
		"@remato/design-system": "^1.7.1",
		"core-js": "^3.20.2",
		"vue": "^3.2.26"
	}
}
